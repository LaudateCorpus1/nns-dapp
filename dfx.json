{
  "canisters": {
    "nns_ui": {
      "type": "custom",
      "candid": "canisters/nns_ui/nns_ui.did",
      "wasm": "target/wasm32-unknown-unknown/release/nns_ui-opt.wasm",
      "build": "./build.sh"
    },
    "nns_ui_assets": {
      "type": "assets",
      "source": ["dfinity_wallet/build/web"]
    }
  },
  "networks": {
    "ic": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    },
    "testnet": {
      "providers": [
        "http://10.12.31.5:8080/"
      ],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:8080",
      "type": "ephemeral"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "version": 1
}